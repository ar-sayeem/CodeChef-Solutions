#include <stdio.h>

int main(void)
{
    int t;
    scanf("%d",&t);
    while(t--)
    {
        char s[6];
        char t[6];
        char temp[6];
        scanf("%s",&s);
        scanf("%s",&t);
        int i=0;
        for(i=0; i<5; i++)
        {
            if(s[i]==t[i])
                temp[i]='G';
            else
                temp[i]='B';
        }
        puts(temp);
    }
    return 0;
}
/*
>> After receiving the input strings s and t, we begin a for loop from 1 to 5 to access each element of the strings.
>> Then we check for equality at each position, inserting 'G' in the output string if it is equal or same, otherwise inserting 'B'.
>> Following the for loop, we print the string using the built-in string function "puts".
*/
